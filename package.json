{
  "name": "social-media-svc",
  "version": "1.0.0",
  "description": "Social Media Service",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run remove-dist",
    "postbuild": "npm run copy-resources",
    "build": "tsc",
    "prestart": "npm run remove-dist",
    "start": "tsc-watch --project ./ --onSuccess \"npm run nodemon\"",
    "startup": "npm run build && node ./dist/src/index.js",
    "prenodemon": "npm run copy-resources",
    "nodemon": "nodemon ./dist/src/index.js",
    "packaging": "npm run build && pkg ./dist/",
    "remove-dist": "rm -rf ./dist",
    "copy-resources": "cp -Rf ./package.json ./dist",
    "pretest": "npm run build",
    "test": "cross-env NODE_ENV=test jest --forceExit --runInBand --detectOpenHandles",
    "watch-test": "yarn run test -- --watchAll",
    "watch-test:ci": "yarn run watch-test -- --coverage",
    "test:c": "yarn run test -- --coverage",
    "precommit": "CI=true yarn lint",
    "lint": "./node_modules/tslint/bin/tslint -c tslint.json -p tsconfig.json --exclude 'node_modules/**/*.{j,t}s'",
    "lint:fix": "yarn lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tanishkul/social-media-svc.git"
  },
  "author": "Tanish Kulshrestha",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tanishkul/social-media-svc/issues"
  },
  "homepage": "https://github.com/tanishkul/social-media-svc#readme",
  "dependencies": {
    "@types/bluebird": "3.5.21",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "1.4.1",
    "@types/cors": "2.8.4",
    "@types/cron": "1.7.2",
    "@types/dotenv": "4.0.3",
    "@types/express": "4.16.0",
    "@types/helmet": "0.0.38",
    "@types/jest": "23.3.1",
    "@types/method-override": "0.0.31",
    "@types/moment-timezone": "0.5.12",
    "@types/mongoose": "5.2.0",
    "@types/node": "13.9.8",
    "@types/swagger-jsdoc": "0.0.2",
    "@types/swagger-ui-express": "3.0.0",
    "@types/winston": "2.3.9",
    "axios": "0.21.1",
    "bcrypt": "^5.0.1",
    "body-parser": "1.18.3",
    "commitizen": "2.10.1",
    "compression": "1.7.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "cross-env": "5.2.0",
    "crypto-js": "^4.0.0",
    "cz-conventional-changelog": "2.1.0",
    "dotenv": "6.0.0",
    "ejs": "^3.1.6",
    "express": "^4.16.3",
    "express-validator": "5.3.0",
    "handlebars": "4.7.7",
    "helmet": "3.23.3",
    "husky": "0.14.3",
    "jest": "23.5.0",
    "jsonwebtoken": "^8.5.1",
    "method-override": "2.3.10",
    "moment-timezone": "0.5.26",
    "mongoose": "5.12.14",
    "morgan-body": "2.4.5",
    "multer": "^1.4.2",
    "multer-gridfs-storage": "^5.0.0",
    "nodemon": "1.18.7",
    "path": "^0.12.7",
    "swagger-jsdoc": "1.9.7",
    "swagger-ui-express": "3.0.10",
    "tsc-watch": "1.0.26",
    "tslint": "5.10.0",
    "typedoc": "0.17.8",
    "typescript": "3.8.3",
    "yarn": "1.12.3"
  }
}
